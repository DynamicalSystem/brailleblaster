<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<pageSettings>
	<!-- Margin and Height based on liblouisutdml epub.cfg -->
	<paperHeight>11</paperHeight>
	<paperWidth>11.5</paperWidth>
	<topMargin>0.5</topMargin>
	<bottomMargin>0.5</bottomMargin>
	<leftMargin>1</leftMargin>
	<rightMargin>0.5</rightMargin>
        
	<begPageNum>1</begPageNum>
